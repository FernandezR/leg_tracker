<?xml version="1.0" ?>
<launch>

  <include file="$(find leg_tracker)/launch/joint_leg_tracker.launch" >
    <arg name="laser_type" value="velodyne" />
    <arg name="local_grid_mapping" value="true"/>
    <arg name="scan_topic" value="velodyne/scan_filtered"/>
    <arg name="fixed_frame" value="velodyne_base_link" />
  </include>

  <include file="$(find leg_tracker)/launch/joint_leg_tracker.launch" >
    <arg name="laser_type" value="hokuyo" />
    <arg name="local_grid_mapping" value="true"/>
    <arg name="scan_topic" value="nav_hokuyo/scan"/>
    <arg name="fixed_frame" value="nav_hokuyo_laser_link" />
  </include>

</launch>
